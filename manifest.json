{  
  
  "name": "Klaviyo Console",
  "version": "1.0",
  "manifest_version": 2,
  "default_locale": "en",  
  "icons": {
    "16": "images/logo.png",
    "32": "images/logo.png",
    "48": "images/logo.png",
    "128":"images/logo.png"
  },  
  
  "browser_action": {
    "default_icon": {
      "19": "images/logo.png",
      "38": "images/logo.png"
    },
    "default_title": "Klaviyo Console",
    "default_popup": "popup.html"
  },   

  "devtools_page": "devtools.html",
   
  "permissions": [
    "http://*/*",
    "https://*/*"    
  ]

}
